{
    "name": "Ibex",
    "version": "1.0",
    "manifest_version":2,
    "content_scripts": [
        {
            "matches":["*://vtop.vit.ac.in/*"],
            "js": ["script.js"]
        }
    ],
    "browser_action": {
    	"default_popup": "popup.html",
    	"default_title": "Ibex",
        "default_icon": "icon_128x128.png"
    },
    "icons": {
        "16": "icon_16x16.png",
        "48": "icon_48x48.png",
        "128": "icon_128x128.png"
    },
    "permissions": [
        "storage"
    ]
}